{
	"schemaVersion": 1,
	"id": "a-better-foods",
	"version": "${version}",
	"name": "A Better Foods",
	"description": "原版食物总是那么的单调，这款模组增加了许多新的食物制作方式和食物！",
	"authors": [
		"xiaomian124"
	],
	"contact": {
		"homepage": "https://github.com/xiaomian124/A-Better-Foods",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "CC0-1.0",
	"icon": "assets/a-better-foods/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"com.xiaomian124.abf.ABetterFoods"
		],
		"client": [
			"com.xiaomian124.abf.ABetterFoodsClient"
		],
		"fabric-datagen": [
			"com.xiaomian124.abf.ABetterFoodsDataGenerator"
		]
	},
	"mixins": [
		"a-better-foods.mixins.json",
		{
			"config": "a-better-foods.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.16.14",
		"minecraft": "~1.20.1",
		"java": ">=17",
		"fabric-api": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
	"custom": {
		"modmenu": {
			"icon": "icon.png"
		}
	}
}
